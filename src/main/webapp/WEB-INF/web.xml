<?xml version="1.0" encoding="UTF-8"?>
<web-app 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" 
	id="WebApp_ID" 
	version="3.0">   
	
    <!-- Lista Serwletow-->
    
    <!-- Rejestracja -->
    <servlet>
    	<servlet-name>Register</servlet-name>
    	<servlet-class>com.psi.project.servlets.Register</servlet-class>
    </servlet>
    
    <!-- Logowanie -->
    <servlet>
    	<servlet-name>Login</servlet-name>
    	<servlet-class>com.psi.project.servlets.Login</servlet-class>
    </servlet>
    
    <!-- Wylogowanie -->
    <servlet>
    	<servlet-name>Logout</servlet-name>
    	<servlet-class>com.psi.project.servlets.Logout</servlet-class>
    </servlet>
    
    <!-- Zmiana Hasła -->
    <servlet>
    	<servlet-name>ChangePassword</servlet-name>
    	<servlet-class>com.psi.project.servlets.ChangePassword</servlet-class>
    </servlet>
    
    <!-- Usunięcie Konta -->
    <servlet>
    	<servlet-name>DeleteAccount</servlet-name>
    	<servlet-class>com.psi.project.servlets.DeleteAccount</servlet-class>
    </servlet>
    
  <!-- Timeout-->
    <session-config>
        <session-timeout>
            5
        </session-timeout>
    </session-config>
</web-app>
